<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.report.vo.HipsActionVO" table="HIPSACTION">
		<id name="hipsActionIdn" column="HIPSACTION_IDN" type="integer" unsaved-value="undefined" />
		<property name="computerIdn" type="integer" column="COMPUTER_IDN" />
		<property name="actionTaken" type="string" column="ACTIONTAKEN" />
		<property name="actionCode" type="integer" column="ACTIONCODE" />
		<property name="actionDate" type="timestamp" column="ACTIONDATE" />
		<property name="application" type="string" column="APPLICATION" />
	</class>
</hibernate-mapping>