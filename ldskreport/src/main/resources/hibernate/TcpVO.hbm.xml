<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.report.vo.TcpVO" table="TCP">
		<id name="networkSoftwareIdn" column="NETWORKSOFTWARE_IDN" type="integer" unsaved-value="undefined" />
		<property name="computerIdn" type="integer" column="COMPUTER_IDN" />
		<property name="address" type="string" column="ADDRESS" />
		<property name="hostName" type="string" column="HOSTNAME" />
	</class>
</hibernate-mapping>